{
  "name": "@lifeyourdreams/monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Life Your Dreams - Immobilienvermarktung Monorepo",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "bootstrap": "pnpm install && pnpm -r build",
    "check:brand": "bash ./scripts/check-lyd-brand.sh",
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "clean": "pnpm -r clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "typescript": "^5.3.3"
  }
}
